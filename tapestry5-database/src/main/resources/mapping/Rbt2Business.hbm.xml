<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.samtech.database">
	<class
		name="Rbt2Business"
		table="rbt2_business"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="string"
			column="biz_cd"
		>
			<generator class="uuid.hex"/>
		</id>

		<property
			name="BizName"
			column="biz_name"
			type="string"
			not-null="true"
			length="100"
		/>
		<property
			name="BizQuestion"
			column="biz_question"
			type="string"
			not-null="false"
			length="500"
		/>
		<property
			name="BizType"
			column="biz_type"
			type="string"
			not-null="true"
			length="20"
		/>
		<property
			name="BizDes"
			column="biz_des"
			type="string"
			not-null="false"
		/>
		<property
			name="BizUrlName"
			column="biz_url_name"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="BizUrl"
			column="biz_url"
			type="string"
			not-null="false"
			length="1000"
		/>
		<property
			name="BizGroupCd"
			column="biz_group_cd"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="LevId"
			column="lev_id"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="IndexWords"
			column="index_words"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="IncludeWords"
			column="include_words"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="ExcludeWords"
			column="exclude_words"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Indexed"
			column="is_indexed"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="Deleted"
			column="deleted"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="OpId"
			column="op_id"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="OpDate"
			column="op_date"
			type="timestamp"
			not-null="false"
			length="23"
		/>


	</class>	
</hibernate-mapping>